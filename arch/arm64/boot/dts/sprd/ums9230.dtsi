		/* apcpu big etf */
		etf@7e003000 {
			compatible = "arm,coresight-tmc", "arm,primecell";
			reg = <0 0x7e003000 0 0x1000>;
			clocks = <&ext_26m>;
			clock-names = "apb_pclk";

