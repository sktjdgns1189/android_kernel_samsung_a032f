				reg = <0x2260000 0x100>;
				interrupts = <GIC_SPI 214 IRQ_TYPE_LEVEL_HIGH>;
				clock-frequency = <400000>;
				#address-cells = <1>;
				#size-cells = <0>;
				status = "disabled";
				syscon-names = "i2c_rst";
				syscons =
					<&ap_ahb_regs REG_AP_AHB_AHB_RST MASK_AP_AHB_I2C2_SOFT_RST>;
			};

			i2c3: i2c@2270000 {
				compatible = "sprd,qogirn6pro-i2c";
				reg = <0x2270000 0x100>;
				interrupts = <GIC_SPI 213 IRQ_TYPE_LEVEL_HIGH>;
				clock-frequency = <400000>;
				#address-cells = <1>;
				#size-cells = <0>;
				status = "disabled";
				syscon-names = "i2c_rst";
				syscons =
					<&ap_ahb_regs REG_AP_AHB_AHB_RST MASK_AP_AHB_I2C3_SOFT_RST>;
